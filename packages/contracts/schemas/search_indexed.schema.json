{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SearchIndexed",
  "description": "Event emitted when an entity is indexed for search",
  "type": "object",
  "properties": {
    "entity": {
      "type": "string",
      "description": "Type of the entity indexed"
    },
    "entity_id": {
      "type": "string",
      "description": "Identifier of the entity indexed"
    }
  },
  "required": ["entity", "entity_id"],
  "additionalProperties": false
}
