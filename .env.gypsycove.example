# GypsyCove Family Dashboard Environment Configuration
# Redis namespace: db=2

# Core
NODE_ENV=production
COOKIE_DOMAIN=.gypsy-cove.xyz
CORS_ORIGINS=https://gypsy-cove.xyz
AUTH_PEPPER=prod_pepper_change_me
AGENT_SHARED_TOKEN=prod_shared_token_64_character_secret_value________
CORE_API_URL=https://api.blackrosecollective.studio

# Redis - GypsyCove Namespace
REDIS_URL=redis://redis:6379/0
REDIS_DB=2  # GypsyCove family dashboard partition

# Agents
NOVA_AGENT_TOKEN=secure_token_change_me

# Postgres
POSTGRES_USER=nova
POSTGRES_PASSWORD=secure_password_change_me
POSTGRES_DB=nova_gypsy
DATABASE_URL=postgresql+psycopg2://nova:secure_password_change_me@db:5432/nova_gypsy

# JWT (RS256)
JWT_ALG=RS256
JWT_PRIVATE_KEY_PATH=/run/secrets/gypsy_jwt_private.pem
JWT_PUBLIC_KEY_PATH=/run/secrets/gypsy_jwt_public.pem

# Platform
PLATFORM_CUT=0.12

# App URLs
CORE_API_BASE=https://api.blackrosecollective.studio
ECHO_WS=wss://echo.blackrosecollective.studio/ws
SITE_URL=https://gypsy-cove.xyz
NOVA_CONSOLE_BASE_URL=https://novaos.blackrosecollective.studio
BRC_DOMAIN=blackrosecollective.studio
